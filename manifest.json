{
    "manifest_version": 3,
    "name": "Input and Tab Saver",
    "version": "1.0",
    "permissions": ["activeTab", "storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    }
}
